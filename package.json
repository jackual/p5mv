{
  "name": "p5mv",
  "productName": "p5mv",
  "version": "0.2.1",
  "private": true,
  "type": "module",
  "main": "main.js",
  "scripts": {
    "generate-sketches": "node scripts/generate-sketches.js",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && NODE_ENV=development electron .",
    "build": "node scripts/generate-sketches.js && vite build",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make --platform=darwin",
    "start": "NODE_ENV=production electron .",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@phosphor-icons/react": "^2.1.10",
    "@vitejs/plugin-react": "^5.1.1",
    "array-equal": "^2.0.0",
    "chroma-js": "^3.1.2",
    "common-js-file-extensions": "^1.0.4",
    "current-module-paths": "^1.1.2",
    "eases": "^1.0.8",
    "fs-extra": "^11.3.2",
    "is-url-external": "^2.0.1",
    "jsdom": "^27.4.0",
    "lodash.capitalize": "^4.2.1",
    "midi-parser-js": "^4.0.4",
    "node-fetch": "^3.3.2",
    "node-zip": "^1.1.1",
    "p5": "^2.0.5",
    "puppeteer": "^24.23.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.64.0",
    "sass": "^1.94.2",
    "skia-canvas": "^3.0.8",
    "valtio": "^2.1.8",
    "vite": "^7.2.4",
    "vulgar-fractions": "^1.5.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "concurrently": "^9.1.0",
    "electron": "^39.2.4",
    "vitest": "^4.0.16",
    "wait-on": "^8.0.1"
  }
}
